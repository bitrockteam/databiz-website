{
  "name": "databiz-website",
  "version": "1.2.0",
  "main": "src/index.js",
  "displayName": "Databiz Group",
  "config": {
    "themeColor": "#0072BC"
  },
  "description": "Technological Innovation Holding Group.",
  "repository": "git@github.com:bitrockteam/databiz-website.git",
  "author": "Salvatore Laisa <salvatore.laisa@bitrock.it>",
  "license": "MIT",
  "scripts": {
    "serve": "node ./scripts/server.js",
    "start": "NODE_ENV=development webpack-dev-server --hot",
    "build:webpack": "rm -rf ./dist && NODE_ENV=production webpack",
    "ssr": "node ./scripts/ssr.js",
    "build": "npm run build:webpack && npm run ssr",
    "deploy": "NODE_ENV=production npm run build && npm run serve",
    "test:unit": "NODE_ENV=test ava ./test/index.js --verbose",
    "test": "npm run test:unit"
  },
  "devDependencies": {
    "ava": "1.0.1",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "2.1.0",
    "cypress": "3.1.4",
    "esm": "3.0.84",
    "express": "4.16.4",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "gh-pages": "2.0.1",
    "html-webpack-plugin": "3.2.0",
    "jsdom": "13.1.0",
    "node-sass": "4.11.0",
    "puppeteer": "1.11.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.28.3",
    "webpack-cli": "3.2.0",
    "webpack-dev-server": "3.1.14",
    "webpack-pwa-manifest": "4.0.0",
    "workbox-webpack-plugin": "3.6.3"
  },
