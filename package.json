    "particles.js": "2.0.0",
    "pwa-helpers": "0.9.0",
    "smoothscroll-polyfill": "0.4.3",
    "timeline": "https://github.com/bitrockteam/timeline",
    "typebase.css": "0.5.0",
    "yarn": "1.12.3"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
