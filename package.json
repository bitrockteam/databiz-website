    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "gh-pages": "1.2.0",
    "html-webpack-plugin": "3.2.0",
    "jsdom": "11.11.0",
    "koa-connect": "2.0.1",
    "node-sass": "4.9.2",
    "puppeteer": "1.8.0",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "webpack": "4.16.1",
